   !s0 && s1 && fork0 && !hasr  -> fork0=false, hasr=true; 
   !s0 && s1 && !fork0 && !hasr  -> s0=s0, s1=s1; 
   !s0 && s1 && hasr && fork5 -> fork5=false, hasl=true;  
   !s0 && s1 && hasr && !fork5 -> fork0=true, hasr=false;
   !s0 && s1 && hasl && hasr -> s0=true, s1=false; 
   s0 && !s1 -> s0=false, s1=false, fork5=true, fork0=true, hasr=false, hasl=false;
   true -> s0=true, s1=true;  
}





Main(){
 phil1:Phil1;
 phil2:Phil2;
 phil3:Phil3;
 phil4:Phil4;
 phil5:Phil5;
 phil6:Phil6;

 run phil1();
 run phil2();
 run phil3();
 run phil4();
 run phil5();
 run phil6();

 

}
