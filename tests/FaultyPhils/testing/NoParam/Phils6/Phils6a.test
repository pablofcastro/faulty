//FIRST RIGHT
Process Phil6{
   s0, s1, hasl, hasr: BOOL;
   Initial: !s0 && !s1  && !hasl && !hasr && fork5 && fork0;
   Normative: !(s0 && s1); 

   !s0 && !s1 -> s0=false, s1=true; 
