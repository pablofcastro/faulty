Process SimpleProtocol{       send:BOOL;       lost:BOOL;       delivered:BOOL;              Initial: !send && !lost && !delivered;       Normative: !lost ;           (!send && !lost && !delivered)-> send=true;           (send && !lost && !delivered)-> delivered=true;           (send && !lost && delivered)-> send=false, delivered=false;}Main(){	p1: SimpleProtocol;	run p1();}